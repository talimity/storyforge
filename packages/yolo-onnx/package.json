{
  "name": "@storyforge/yolo-onnx",
  "description": "Convenience package for yolov8n-face focal point detection via ONNX",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",

    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "detect": "tsx src/test-face-detection.ts",
    "fmt": "biome format --write .",
    "lint": "biome check --diagnostic-level error --write --unsafe .",
    "typecheck": "tsgo -p tsconfig.json --noEmit",
    "typecheck:legacy": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "onnxruntime-node": "1.23.0",
    "sharp": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "@typescript/native-preview": "catalog:"
  }
}
